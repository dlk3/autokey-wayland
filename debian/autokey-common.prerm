#!/bin/sh

set -e

case "$1" in
    remove|deconfigure)
        rm -fr '/usr/share/gnome-shell/extensions/autokey-gnome-extension@autokey'
        rm '/etc/udev/rules.d/10-autokey.rules'
        /usr/bin/udevadm control --reload
        /usr/bin/udevadm trigger /dev/uinput
    ;;
esac

#DEBHELPER#

exit 0
